generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model User {
  key       Int    @id @default(autoincrement())
  id        String @unique
  email     String
  fullname  String
  avatarUrl String
}
