generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Brand {
  key      Int    @id @default(autoincrement())
  id       String @unique
  email    String
  name     String
  location String
}

model Look {
  key Int    @id @default(autoincrement())
  id  String @unique
}
